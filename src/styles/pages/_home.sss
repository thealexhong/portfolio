.Home
  min-height: 100vh

  .Home-header
    display: flex
    flex-flow: column wrap
    justify-content: center
    align-items: center

  .Home-logo
    margin-top: 50px
    padding: 1.5rem

  h1,h3,p
    text-align: center

  h1,h3
    font-family: "Proxima Nova Soft Bold"

  h1
    font-size: 1.5rem
    padding: 0.5rem

  h3
    font-size: 1.2rem
    padding: 0.5rem

  p
    padding: 0.25rem

.Home-footer
  font-size: 0.8rem
  margin-top: 2rem
  padding: 1rem




